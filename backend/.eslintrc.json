{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "implicit-arrow-linebreak": ["error", "below"],
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "no-else-return":"off",
        "no-underscore-dangle":"off"
    }
}
